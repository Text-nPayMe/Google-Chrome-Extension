{
  "manifest_version": 2,
  "name": "V Launcher",
  "description": "My Google Chrome Extension",
  "version": "1.0.0",
  "icons": {
    "128": "images/logo-128.png"
  },
  "browser_action": {
    "default_icon": "images/logo.png",
    "default_popup": "index.html",
    "default_title": "Click Me"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "cookies",
    "search",
    "storage",
    "pageCapture"
  ]
}
